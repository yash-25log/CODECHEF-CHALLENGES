#include<bits/stdc++.h>
#define ll long long int
using namespace std;
#define fast ios_base::sync_with_stdio(false);cin.tie(0);cout.tie(0);

int main(){

    fast

    int t;
    cin>>t;

    while(t--){
        int k;
        cin>>k;
        int cnt=0;
        while(k%2==0){
            cnt++;
            k/=2;
        }
        cout<<cnt<<endl;

    }

    return 0;
}
